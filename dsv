        await bot.send_message(callback_query.from_user.id, f'Z908494970337 –ü–æ–ø–æ–ª–Ω–∏—Ç–µ —ç—Ç–æ—Ç —Å—á–µ—Ç —á–µ—Ä–µ–∑ WebMoney –∏ –æ—Ç–ø—Ä–∞–≤—å—Ç–µ –Ω–∞–º —Å–∫—Ä–∏–Ω—à–æ—Ç –æ–± –æ–ø–ª–∞—Ç–µ. –°—É–º–º–∞: {price} —Å—É–º')
    elif payment_method == 'qiwiuz':
        await bot.send_message(callback_query.from_user.id, f'P1098178866 –ü–æ–ø–æ–ª–Ω–∏—Ç–µ —ç—Ç–æ—Ç —Å—á—ë—Ç —á–µ—Ä–µ–∑ Payeer –∏ –æ—Ç–ø—Ä–∞–≤—å—Ç–µ —Å—é–¥–∞ —Å–∫—Ä–∏–Ω—à–æ—Ç –æ–± –æ–ø–ª–∞—Ç–µ. –°—É–º–º–∞: {price} —Å—É–º')

@dp.message_handler(content_types=['photo'])
async def photo_handler(message: types.Message):
    await message.answer('–ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ–ø–æ–ª–Ω–µ–Ω–∏—è —Å—á–µ—Ç–∞, –ø–æ–¥–æ–∂–¥–∏—Ç–µ.. –° –≤–∞–º–∏ —Å–≤—è–∂–µ—Ç—Å—è –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä –ø—Ä–∏ —É—Å–ø–µ—à–Ω–æ–π –ø—Ä–æ–≤–µ—Ä–∫–µ –∏ –ø–µ—Ä–µ–¥–∞—Å—Ç –∞–∫–∫–∞—É–Ω—Ç')

@dp.callback_query_handler(lambda c: c.data == 'history')
async def history_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    user_id = callback_query.from_user.id
    if user_id in user_data and 'history' in user_data[user_id]:
        history = user_data[user_id]['history']
        if len(history) > 0:
            history_text = '–í–∞—à–∞ –∏—Å—Ç–æ—Ä–∏—è –ø–æ–∫—É–ø–æ–∫:\n\n'
            for item in history:
                history_text += f'–¶–µ–Ω–∞: {item["price"]} —Å—É–º, –°–ø–æ—Å–æ–± –æ–ø–ª–∞—Ç—ã: {item["payment_method"]}\n'
            await bot.send_message(callback_query.from_user.id, history_text)
        else:
            await bot.send_message(callback_query.from_user.id, '–£ –≤–∞—Å –Ω–µ—Ç –∏—Å—Ç–æ—Ä–∏–∏ –ø–æ–∫—É–ø–æ–∫')
    else:
        await bot.send_message(callback_query.from_user.id, '–£ –≤–∞—Å –Ω–µ—Ç –∏—Å—Ç–æ—Ä–∏–∏ –ø–æ–∫—É–ø–æ–∫')

@dp.callback_query_handler(lambda c: c.data == 'back_start')
async def back_start_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    keyboard = types.InlineKeyboardMarkup()
    keyboard.add(types.InlineKeyboardButton('–¶–µ–Ω—ã', callback_data='prices'), 
                 types.InlineKeyboardButton('–ö—É–ø–∏—Ç—å', callback_data='buy'),
                 types.InlineKeyboardButton('–ò—Å—Ç–æ—Ä–∏—è –ø–æ–∫—É–ø–æ–∫', callback_data='history'))
    await bot.edit_message_text(chat_id=callback_query.message.chat.id, message_id=callback_query.message.message_id, text='–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π! –ó–¥–µ—Å—å –≤—ã –º–æ–∂–µ—Ç–µ –ø—Ä–∏–æ–±—Ä–µ—Å—Ç–∏ –∞–∫–∫–∞—É–Ω—Ç—ã –¥–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –≤ –¢–µ–ª–µ–≥—Ä–∞–º üõí', reply_markup=keyboard)

@dp.callback_query_handler(lambda c: c.data == 'back_prices')
async def back_prices_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    keyboard = types.InlineKeyboardMarkup()
