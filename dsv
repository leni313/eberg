@dp.message_handler(commands=['help'])
async def send_help(message: types.Message):
    keyboard = types.InlineKeyboardMarkup()
    keyboard.add(types.InlineKeyboardButton("–°–≤—è–∑–∞—Ç—å—Å—è‚¨ÜÔ∏è", url="http://t.me/Martketplace_buy_FAQ"))
    keyboard.add(types.InlineKeyboardButton("–ü–æ–ø—Ä–æ—Å–∏—Ç—å —Å–≤—è–∑–∞—Ç—å—Å—è ‚¨áÔ∏è", callback_data='request_contact'))
    await message.answer("–£ –≤–∞—Å –≤–æ–∑–Ω–∏–∫–ª–∏ –∫–∞–∫–∏–µ-–ª–∏–±–æ –≤–æ–ø—Ä–æ—Å—ã? –í—ã –º–æ–∂–µ—Ç–µ —Å–≤—è–∑–∞—Ç—å—Å—è —Å –Ω–∞–º–∏ –∏–ª–∏ –∂–µ –ø–æ–ø—Ä–æ—Å–∏—Ç—å —Å–≤—è–∑–∞—Ç—å—Å—è", reply_markup=keyboard)

@dp.callback_query_handler(lambda c: c.data == 'request_contact')
async def process_callback_request_contact(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    await bot.send_message(-1001690807093, f"–ü–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—å {callback_query.from_user.full_name} –ø–æ–ø—Ä–æ—Å–∏–ª —Å–≤—è–∑–∞—Ç—å—Å—è")
    await bot.send_message(callback_query.from_user.id, "–°–ø–∞—Å–∏–±–æ –∑–∞ –∑–∞—è–≤–∫—É! –ú—ã —Å–≤—è–∂–µ–º—Å—è —Å –≤–∞–º–∏ –≤ —Å–∫–æ—Ä–æ–º –≤—Ä–µ–º–µ–Ω–∏. –ï—Å–ª–∏ –≤–∞—à –∞–∫–∫–∞—É–Ω—Ç –Ω–µ –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –≤ –°–ü–ê–ú–µ, –≤—ã —Ç–∞–∫–∂–µ –º–æ–∂–µ—Ç–µ —Å–≤—è–∑–∞—Ç—å—Å—è —Å –Ω–∞–º–∏ —Å–∞–º–æ—Å—Ç–æ—è—Ç–µ–ª—å–Ω–æ.\n\n–ù–∞–∂–º–∏—Ç–µ /start, —á—Ç–æ–±—ã –≤–µ—Ä–Ω—É—Ç—å—Å—è –≤ –≥–ª–∞–≤–Ω–æ–µ –º–µ–Ω—é.")

keep_alive()
if __name__ == '__main__':
    executor.start_polling(dp)
