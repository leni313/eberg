from aiogram import Bot, Dispatcher, executor, types
from background import keep_alive

bot_token = "6367461912:AAHcx6D5UQ_MELsVV3pcFnpgDeIt2ZpMgD8"
PROXY_URL = 'https://209.97.150.167:3128'
group_id = -1001690807093

bot = Bot(token=bot_token)
dp = Dispatcher(bot)

user_data = {}

@dp.message_handler(commands=['start'])
async def start_cmd_handler(message: types.Message):
    keyboard = types.InlineKeyboardMarkup()
    keyboard.add(types.InlineKeyboardButton('–¶–µ–Ω—ã', callback_data='prices'), 
                 types.InlineKeyboardButton('–ö—É–ø–∏—Ç—å', callback_data='buy'),
                 types.InlineKeyboardButton('–ò—Å—Ç–æ—Ä–∏—è –ø–æ–∫—É–ø–æ–∫', callback_data='history'))
    sent_message = await message.answer('–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π! –ó–¥–µ—Å—å –≤—ã –º–æ–∂–µ—Ç–µ –ø—Ä–∏–æ–±—Ä–µ—Å—Ç–∏ –∞–∫–∫–∞—É–Ω—Ç—ã –¥–ª—è –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏—è –≤ –¢–µ–ª–µ–≥—Ä–∞–º üõí', reply_markup=keyboard)
    await sent_message.pin()

@dp.callback_query_handler(lambda c: c.data == 'prices')
async def prices_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    keyboard = types.InlineKeyboardMarkup()
    keyboard.add(types.InlineKeyboardButton('–ö—É–ø–∏—Ç—å', callback_data='buy'), 
                 types.InlineKeyboardButton('–ù–∞–∑–∞–¥', callback_data='back_start'))
    await bot.edit_message_text(chat_id=callback_query.message.chat.id, message_id=callback_query.message.message_id, text='üáÆüá© +62 –ò–Ω–¥–æ–Ω–µ–∑–∏—è - 15000 —Å—É–º \nüá∑üá∫ +7 –†–æ—Å—Å–∏—è - 23000 —Å—É–º \nüá∫üá≤ +1 –°–®–ê - 24000 —Å—É–º \nüá∫üá¶ +380 –£–∫—Ä–∞–∏–Ω–∞ - 25000 —Å—É–º \nüáÆüáπ +39 –ò—Ç–∞–ª–∏—è - 22000 —Å—É–º \nüá®üáµ +33 –§—Ä–∞–Ω—Ü–∏—è - 22000 —Å—É–º\nüá∫üáø +998 –£–∑–±–µ–∫–∏—Å—Ç–∞–Ω - 15 000 —Å—É–º \n \n–ú—ã –ø—Ä–∏–Ω–∏–º–∞–µ–º –≤—Å–µ –ø–æ–ø—É–ª—è—Ä–Ω—ã–µ –º–µ—Ç–æ–¥—ã –æ–ø–ª–∞—Ç—ã: Click, Payme, Oson, Paynet, WebMoney, Qiwi Uz', reply_markup=keyboard)

@dp.callback_query_handler(lambda c: c.data == 'buy')
async def buy_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    keyboard = types.InlineKeyboardMarkup()
    keyboard.add(types.InlineKeyboardButton('üáÆüá© +62 –ò–Ω–¥–æ–Ω–µ–∑–∏—è - 15000 —Å—É–º', callback_data='price_15000'), 
                 types.InlineKeyboardButton('üá∑üá∫ +7 –†–æ—Å—Å–∏—è - 23000 —Å—É–º', callback_data='price_23000'),
                 types.InlineKeyboardButton('üá∫üá≤ +1 –°–®–ê - 24000 —Å—É–º', callback_data='price_24000'),
                 types.InlineKeyboardButton('üá∫üá¶ +380 –£–∫—Ä–∞–∏–Ω–∞ - 25000 —Å—É–º', callback_data='price_25000'),
                 types.InlineKeyboardButton('üáÆüáπ +39 –ò—Ç–∞–ª–∏—è - 22000 —Å—É–º', callback_data='price_22000'),
                 types.InlineKeyboardButton('üá®üáµ +33 –§—Ä–∞–Ω—Ü–∏—è - 22000 —Å—É–º', callback_data='price_22000'),
                 types.InlineKeyboardButton('üá∫üáø +998 –£–∑–±–µ–∫–∏—Å—Ç–∞–Ω - 15 000 —Å—É–º', callback_data='price_15000'),
                 types.InlineKeyboardButton('–ù–∞–∑–∞–¥', callback_data='back_prices'))
    await bot.edit_message_text(chat_id=callback_query.message.chat.id, message_id=callback_query.message.message_id, text='–í—ã–±–µ—Ä–∏—Ç–µ –∫–æ–¥ –Ω–æ–º–µ—Ä–∞', reply_markup=keyboard)

@dp.callback_query_handler(lambda c: c.data.startswith('price_'))
async def price_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    user_id = callback_query.from_user.id
    if user_id not in user_data:
        user_data[user_id] = {'history': []}
    user_data[user_id]['price'] = callback_query.data.split('_')[1]
    keyboard = types.InlineKeyboardMarkup()
    keyboard.add(types.InlineKeyboardButton('Click', callback_data='payment_click'), 
                 types.InlineKeyboardButton('Payme', callback_data='payment_payme'),
                 types.InlineKeyboardButton('WebMoney', callback_data='payment_webmoney'),
                 types.InlineKeyboardButton('Qiwi Uz', callback_data='payment_qiwiuz'),
                 types.InlineKeyboardButton('–ù–∞–∑–∞–¥', callback_data='back_buy'))
    await bot.edit_message_text(chat_id=callback_query.message.chat.id, message_id=callback_query.message.message_id, text='–í—ã–±–µ—Ä–∏—Ç–µ —Å–ø–æ—Å–æ–± –æ–ø–ª–∞—Ç—ã', reply_markup=keyboard)

@dp.callback_query_handler(lambda c: c.data.startswith('payment_'))
async def payment_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    user_id = callback_query.from_user.id
    payment_method = callback_query.data.split('_')[1]
    price = user_data[user_id]['price']
    user_data[user_id]['history'].append({'price': price, 'payment_method': payment_method})
    if payment_method == 'click':
        await bot.send_message(callback_query.from_user.id, f'8802405219113439 –ü–µ—Ä–µ–∫–∏–Ω—å—Ç–µ –Ω–∞ —ç—Ç–æ—Ç —Å—á–µ—Ç —á–µ—Ä–µ–∑ Click –∏ –æ—Ç–ø—Ä–∞–≤—å—Ç–µ –Ω–∞–º —Å–∫—Ä–∏–Ω—à–æ—Ç –æ –ø–µ—Ä–µ–≤–æ–¥–µ. –°—É–º–º–∞: {price} —Å—É–º')
    elif payment_method == 'payme':
        await bot.send_message(callback_query.from_user.id, f'2505018609035827 –ü–æ–ø–æ–ª–Ω–∏—Ç–µ —ç—Ç–æ—Ç —Å—á–µ—Ç —á–µ—Ä–µ–∑ Payme –∏ –æ—Ç–ø—Ä–∞–≤—å—Ç–µ –Ω–∞–º —Å–∫—Ä–∏–Ω—à–æ—Ç –æ–± –æ–ø–ª–∞—Ç–µ. –°—É–º–º–∞: {price} —Å—É–º')
    elif payment_method == 'webmoney':
        await bot.send_message(callback_query.from_user.id, f'Z908494970337 –ü–æ–ø–æ–ª–Ω–∏—Ç–µ —ç—Ç–æ—Ç —Å—á–µ—Ç —á–µ—Ä–µ–∑ WebMoney –∏ –æ—Ç–ø—Ä–∞–≤—å—Ç–µ –Ω–∞–º —Å–∫—Ä–∏–Ω—à–æ—Ç –æ–± –æ–ø–ª–∞—Ç–µ. –°—É–º–º–∞: {price} —Å—É–º')
    elif payment_method == 'qiwiuz':
        await bot.send_message(callback_query.from_user.id, f'P1098178866 –ü–æ–ø–æ–ª–Ω–∏—Ç–µ —ç—Ç–æ—Ç —Å—á—ë—Ç —á–µ—Ä–µ–∑ Payeer –∏ –æ—Ç–ø—Ä–∞–≤—å—Ç–µ —Å—é–¥–∞ —Å–∫—Ä–∏–Ω—à–æ—Ç –æ–± –æ–ø–ª–∞—Ç–µ. –°—É–º–º–∞: {price} —Å—É–º')

@dp.message_handler(content_types=['photo'])
async def photo_handler(message: types.Message):
    await message.answer('–ü—Ä–æ–≤–µ—Ä–∫–∞ –ø–æ–ø–æ–ª–Ω–µ–Ω–∏—è —Å—á–µ—Ç–∞, –ø–æ–¥–æ–∂–¥–∏—Ç–µ.. –° –≤–∞–º–∏ —Å–≤—è–∂–µ—Ç—Å—è –∞–¥–º–∏–Ω–∏—Å—Ç—Ä–∞—Ç–æ—Ä –ø—Ä–∏ —É—Å–ø–µ—à–Ω–æ–π –ø—Ä–æ–≤–µ—Ä–∫–µ –∏ –ø–µ—Ä–µ–¥–∞—Å—Ç –∞–∫–∫–∞—É–Ω—Ç')

@dp.callback_query_handler(lambda c: c.data == 'history')
async def history_handler(callback_query: types.CallbackQuery):
    await bot.answer_callback_query(callback_query.id)
    user_id = callback_query.from_user.id
    if user_id in user_data and 'history' in user_data[user_id]:
        history = user_data[user_id]['history']
        if len(history) > 0:
            history_text = '–í–∞—à–∞ –∏—Å—Ç–æ—Ä–∏—è –ø–æ–∫—É–ø–æ–∫:\n\n'
            for item in history:
                history_text += f'–¶–µ–Ω–∞: {item["price"]} —Å—É–º, –°–ø–æ—Å–æ–± –æ–ø–ª–∞—Ç—ã: {item["payment_method"]}\n'
            await bot.send_message(callback_query.from_user.id, history_text)
        else:
            await bot.send_message(callback_query.from_user.id, '–£ –≤–∞—Å –Ω–µ—Ç –∏—Å—Ç–æ—Ä–∏–∏ –ø–æ–∫—É–ø–æ–∫')
    else:
        await bot.send_message(callback_query.from_user.id, '–£ –≤–∞—Å –Ω–µ—Ç –∏—Å—Ç–æ—Ä–∏–∏ –ø–æ–∫—É–ø–æ–∫')

